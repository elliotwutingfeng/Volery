{
  "python.formatting.provider": "black",
  "[python]": {
    "editor.formatOnSave": true
  },
  "[sql]": {
    "editor.formatOnSave": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": ["javascript"],
  "Prettier-SQL.SQLFlavourOverride": "postgresql"
}
